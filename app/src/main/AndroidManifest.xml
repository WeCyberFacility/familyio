<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qooplite.alpay.familyio">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/familyiologo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/familyiologo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EinstellungenTab"></activity>
        <activity android:name=".familieTab" />
        <activity
            android:name=".AddFamilieTab"
            android:screenOrientation="landscape" />
        <activity
            android:name=".kochbuchUnterTab"
            android:screenOrientation="landscape" />
        <activity
            android:name=".EssenTab"
            android:screenOrientation="landscape" />
        <activity
            android:name=".SireneTab"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Home"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>